<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Configurations>Debug;Release;Prefetch Cleaner</Configurations>
    <Platforms>AnyCPU;x64</Platforms>
    <ApplicationIcon>Prefetch Cleaner.ico</ApplicationIcon>
    <Title>Prefetch Cleaner</Title>
    <Company>$(David Amato)</Company>
    <Product>$(Prefetch Cleaner)</Product>
    <Copyright>David Amato. All Rights Reserved.</Copyright>
    <PackageIcon>Prefetch Cleaner.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Prefetch Cleaner.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\Users\biggr\Desktop\Prefetch Cleaner.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
